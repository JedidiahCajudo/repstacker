<h1><%= @workout.date %></h1>
<% @workout.exercises.each do |exercise|  %>
  <%= exercise.name %>
  <% exercise.working_sets.each do |set| %>
    <li>
      <%= set.reps %> reps @ <%= set.weight %> kg
    </li>
  <% end %>
<% end %>
<%= render "form", workout: @workout %>
<%= button_to "Delete Workout", @workout, method: :delete, data: { turbo_confirm: "Are you sure?"} %>
<%= link_to "Back", workouts_path %>
